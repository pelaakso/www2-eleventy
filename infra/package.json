{
  "name": "www2-11ty-infra",
  "version": "1.0.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "all": "npm run clean && npx eslint . && npx prettier --check . && npm run test",
    "build": "tsc --noEmit",
    "clean": "npx rimraf '+(bin|lib|functions|test|utils)/**/+(*.d.ts|*.js)'",
    "watch": "tsc -w",
    "test": "npm run build && jest",
    "snap": "jest",
    "cdk": "npx cdk"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.76.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.76.0-alpha.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.5",
    "aws-cdk": "^2.76.0",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.76.0",
    "constructs": "^10.2.1",
    "source-map-support": "^0.5.21"
  }
}
