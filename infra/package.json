{
  "name": "www2-11ty-infra",
  "version": "1.0.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "all": "npm run clean && npx eslint . && npx prettier --check . && npm run test",
    "build": "tsc --noEmit",
    "clean": "npx rimraf '+(bin|lib|src|test)/**/+(*.d.ts|*.js)'",
    "watch": "tsc -w",
    "test": "npm run build && jest",
    "snap": "jest",
    "cdk": "npx cdk"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.86.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.86.0-alpha.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "aws-cdk": "2.99.0",
    "esbuild": "^0.18.11",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.86.0",
    "constructs": "^10.2.47",
    "source-map-support": "^0.5.21"
  }
}
