{
  "name": "www2-11ty-infra",
  "version": "1.0.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "engines": {
    "node": ">=16.0.0 <17",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "all": "npm run clean && npx eslint . && npx prettier --check . && npm run test",
    "build": "tsc --noEmit",
    "clean": "npx rimraf '+(bin|lib|functions|test|utils)/**/+(*.d.ts|*.js)'",
    "watch": "tsc -w",
    "test": "npm run build && jest",
    "snap": "jest",
    "cdk": "npx cdk"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.50.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.50.0-alpha.0",
    "@types/aws-lambda": "^8.10.108",
    "@types/jest": "^29.2.0",
    "@types/node": "^16.18.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.42.0",
    "aws-cdk": "^2.50.0",
    "esbuild": "^0.15.13",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.50.0",
    "constructs": "^10.1.151",
    "source-map-support": "^0.5.21"
  }
}
