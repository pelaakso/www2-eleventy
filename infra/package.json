    "@types/aws-lambda": "^8.10.100",
    "@types/jest": "^28.1.3",
    "@types/node": "^16.11.41",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.29.0",
    "aws-cdk": "^2.28.1",
    "esbuild": "^0.14.47",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.28.1",
